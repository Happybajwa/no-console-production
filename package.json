{
  "name": "no-console-production",
  "version": "1.0.6",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.prod.json",
    "build:playground": "esbuild playground/playground.tsx --bundle --outfile=dist/playground.js --loader:.tsx=tsx --define:process.env.NODE_ENV='development'",
    "start": "http-server . -c-1"
  },
  "keywords": [
    "console",
    "suppress",
    "React",
    "production",
    "logging"
  ],
  "author": "Harry",
  "license": "MIT",
  "description": "A React component to suppress console logs, warnings, errors, and debug messages in production.",
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "esbuild": "^0.24.0",
    "eslint": "^9.12.0",
    "http-server": "^14.1.1",
    "react": "^18.3.1",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "react-dom": "^18.3.0"
  }
}
